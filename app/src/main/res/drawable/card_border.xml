<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimaryDark" />
            <corners android:radius="@dimen/seven_dp" />
        </shape>
    </item>
    <item android:top="@dimen/four_dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimaryDark" />
            <corners android:radius="@dimen/seven_dp" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/onepointfive_dp"
        android:left="@dimen/onepointfive_dp"
        android:right="@dimen/onepointfive_dp"
        android:top="@dimen/four_dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorWhite" />
            <corners android:radius="@dimen/seven_dp" />
        </shape>
    </item>
</layer-list>

